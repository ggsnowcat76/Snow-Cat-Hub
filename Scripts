if not game:IsLoaded() then game.Loaded:Wait() end

local games = {
    [6701277882] = 'https://api.luarmor.net/files/v3/loaders/eb702c1c43441741e407724772f756f3.lua',
    [121864768012064] = 'https://api.luarmor.net/files/v3/loaders/eb702c1c43441741e407724772f756f3.lua',
}

if games[game.PlaceId] then
    loadstring(game:HttpGet(games[game.PlaceId]))()
else
    warn("⚠️ Game not supported:", game.PlaceId)
end
